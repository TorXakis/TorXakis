PROCDEF ap2[In :: Int; Out :: String # String]() ::=
        In ? v
    >-> Out ! "Boom" ! "Bang" [[v == 0]]
    >-> In ? w
    >-> Out ! "Uh?" ! "Ah!" [[v == 1]]
ENDDEF

PROCDEF ap2[In :: Int; Out :: String # String](e :: String) HIT ::=
        In ? v
    >-> Out ! "Boom" ? z [[z == e]]
    >-> HIT
    >-> In ? w
    >-> Out ! "Uh?" ! "Ah!" [[v == 1]]
ENDDEF

PROCDEF ap2[In :: Int; Out :: String # String]() HIT ::=
