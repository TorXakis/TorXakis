TYPEDEF  Operation ::=
      Plus  { p1, p2 :: Int }
      -- Hello!
    | Minus { m1, m2 :: Int }
ENDDEF

FUNCDEF overflow (x :: Bool) :: Bool ::=
        x
ENDDEF

FUNCDEF foo (op :: Operation) :: Operation ::=
        op
ENDDEF
