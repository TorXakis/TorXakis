-- ----------------------------------------------------------------------------------------- --
-- channels

CHANDEF  ChanDefs   ::=  In, Out  :: Int
ENDDEF


-- ----------------------------------------------------------------------------------------- --
-- inputs and outputs

PROCDEF  proc  [ In, Out :: Int ]  ( ) ::=
        In ? x
    >-> Out ! x
    >-> proc [ In, Out ] ()
ENDDEF

-- ----------------------------------------------------------------------------------------- --
-- model


MODELDEF  Model ::=
        CHAN IN     In
        CHAN OUT    Out

        BEHAVIOUR   proc [ In, Out ] ( )
ENDDEF


