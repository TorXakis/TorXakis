-- ----------------------------------------------------------------------------------------- --
-- ProcDefs to test LPE transformation
-- ----------------------------------------------------------------------------------------- --

CHANDEF  Chans  ::=    A, B :: Int
ENDDEF

PROCDEF  p  [ A :: Int] ( ) ::=
       A ?x >-> STOP |[A]| A ?x >-> STOP
ENDDEF


MODELDEF  P ::=
    CHAN IN    A
    BEHAVIOUR  p [A]()
ENDDEF
