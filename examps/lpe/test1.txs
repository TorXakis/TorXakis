-- ----------------------------------------------------------------------------------------- --
-- ProcDefs to test LPE transformation
-- ----------------------------------------------------------------------------------------- --

CHANDEF  Chans  ::=    A, B :: Int
ENDDEF

PROCDEF  p  [ A :: Int;  B :: Int ] ( ) ::=
       A ?x [[ x > 10 ]]  >->  B !x  >->  p [ A, B ] ( )
    ##
       A ?x [[ x <= 10]]  >->  B !(x+1)  >->  p [ A, B ] ( )
ENDDEF


MODELDEF  P ::=
    CHAN IN    A
    CHAN OUT   B
    BEHAVIOUR  p [ A, B ] ( )
ENDDEF
