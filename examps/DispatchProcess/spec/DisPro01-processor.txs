-- ----------------------------------------------------------------------------------------- --
-- Dispatch Process


PROCDEF  processor [ Start, Finish ] ( ) ::=
             Start
        >->  Finish
        >->  processor [ Start, Finish ] ( )
ENDDEF


-- ----------------------------------------------------------------------------------------- --


MODELDEF Model ::=
    CHAN IN    Start
    CHAN OUT   Finish

    BEHAVIOUR

        processor [ Start, Finish] ( )

ENDDEF


-- ----------------------------------------------------------------------------------------- --

CHANDEF Channels ::=
	Start, Finish
ENDDEF

